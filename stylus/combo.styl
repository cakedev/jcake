.jcake-combo-wrapper
  display: inline-block
  font-family: font_family

  // IE7 hack
  zoom: 1
  *display: inline

  select
    display: none

  .jcake-combo
    rounded(3px)

    background-color: #eee
    border: 1px solid #cacaca
    cursor: pointer
    position: relative

    &:hover
      background-color: #3c78b5
      border-color: #125090
      color: #fff

      .jcake-combo-text
        border-color: #125090
        text-shadow: 0px -1px 0px #333

      .jcake-combo-arrow
        border-color: #7698bc

    &:active
      shadow(inside 0px 0px 15px 0px rgba(0, 0, 0, 0.4))

    .jcake-combo-text
      border-right: 1px solid #cacaca
      font-size: 13px
      font-weight: bold
      margin-right: 20px
      overflow: hidden
      padding: 4px 15px
      text-overflow: ellipsis
      text-shadow: 0px 1px 0px #fff
      white-space: nowrap

    .jcake-combo-arrow
      border-left: 1px solid #fff
      height: 100%
      position: absolute
      right: 0px
      top: 0px
      width: 19px

  .jcake-panel
    margin-top: 3px

    .jcake-panel-header
      height: 35px
      padding: 0px 15px

      h1
        line-height: 35px

      .jcake-close-button
        height: 35px

    .jcake-combo-panel
      .jcake-combo-search-box
        background-color: #f5f5f5;
        border-bottom: 1px solid #ddd;
        height: 50px;
        line-height: 50px;
        padding: 0px 10px;
        text-align: center;

        .jcake-combo-search
          animate(border 200ms, box-shadow 200ms)
          box_sizing(border-box)
          rounded(4px)
          shadow(0 0 0 0 rgba(0, 0, 0, 0), inside 0 1px 1px 0 rgba(0, 0, 0, 0.075))

          border: 1px solid #ccc
          min-width: 250px
          padding: 8px
          width: 100%

          &:focus
            shadow(0px 0px 8px 0px rgba(82, 168, 236, 0.6), inside 0px 1px 1px 0px rgba(0, 0, 0, 0.075))

            border-color: rgba(82, 168, 236, 0.8)
            outline: 0

      .jcake-combo-items
        rounded(0px 0px 6px 6px)

        max-height: 300px
        overflow: auto
        position: relative

        .jcake-combo-item
          background-image: url(unquote('data:image/png;base64,'))
          background-repeat: no-repeat
          background-position: 15px center
          border-bottom: 1px solid #eee
          color: #555
          cursor: pointer
          display: block
          font-size: 12px
          height: 32px
          line-height: 32px
          padding: 0px 30px 0px 46px
          white-space: nowrap

          &.jcake-combo-item-selected
            background-image: url(unquote('data:image/png;base64,'))
            color: #333
            font-weight: bold

          &.jcake-combo-item-hover
            background-color: #3c78b5
            background-image: url(unquote('data:image/png;base64,'))
            color: #fff

          &.jcake-combo-item-hidden
            display: none
